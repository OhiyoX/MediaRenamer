{
  "name": "日式剧集格式",
  "pattern": "(.+?)\\.(\\d{4})\\.第(\\d+)話\\.(\\d+x\\d+)\\.(.+?)\\.x264\\.AC3",
  "groups": {
    "title": 1,
    "year": 2,
    "episode": 3,
    "resolution": 4,
    "quality": 5
  },
  "output_format": "{title} ({year}) S01E{episode:0>2} - 第{episode}話.{resolution}.{quality}.x264.AC3"
}