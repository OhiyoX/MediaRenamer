{
  "name": "CASO特殊格式",
  "pattern": "\\[CASO\\]\\[([^\\]]+)\\]\\[([^\\d][^\\]]*)\\]\\[([^\\]]+)\\]\\[([^\\]]+)\\]\\[([^\\]]+)\\]\\[([^\\]]+)\\]",
  "groups": {
    "series_name": 1,
    "episode": 2,
    "quality": 3,
    "resolution": 4,
    "codec": 5,
    "hash": 6
  },
  "output_format": "{series_name} {episode} - [CASO][{quality}][{resolution}][{codec}]"
}