{
  "name": "HYSUB普通格式",
  "pattern": "\\[HYSUB\\]([^\\[]+)\\[(\\d+(?:v\\d+)?)\\]\\[([^\\]]+)\\]\\[([^\\]]+)\\]",
  "groups": {
    "series_name": 1,
    "episode": 2,
    "language": 3,
    "resolution": 4
  },
  "output_format": "{series_name} S01E{episode:0>2} - [HYSUB][{language}][{resolution}]"
}