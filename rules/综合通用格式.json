{
  "name": "综合通用格式",
  "pattern": "^(?:\\[(?P<sub_group>[^\\]]+)\\]\\s*)?(?P<series>[^\\s\\[\\]-]+?)?\\s*(?:S(?P<season>\\d{1,2}))?\\s*(?:E(?P<episode_se>\\d{1,3}))?\\s*(?:-|\\.)?\\s*(?P<title>[^\\[]+?)?(?:\\s*(?P<tech_info>\\[[^\\]]+\\]))?(?:\\.(?P<extension>mp4|mkv|avi|mov|wmv|flv|webm|m4v|mpg|mpeg|3gp|ogv|ts|m2ts))?$",
  "groups": {
    "sub_group": 1,
    "series": 2,
    "season": 3,
    "episode_se": 4,
    "title": 5,
    "tech_info": 6,
    "extension": 7
  },
  "output_format": "{series} S{season:0>2}E{episode:0>2} - {title} {tech_info}",
  "special_handling": {
    "fallback_fields": {
      "season": "01",
      "episode": ["episode_se", "01"],
      "series": ["series", "未知系列"],
      "title": ["title", ""]
    },
    "year_pattern": "(19|20)\\d{2}",
    "max_episode": 999
  }
}


