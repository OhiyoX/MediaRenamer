{
  "name": "通用字幕组格式",
  "pattern": "\\[(.+?)\\]\\[(.+?)\\]\\[(.+?)\\]\\[(.+?)\\]\\[(.+?)\\]\\.(.+)",
  "groups": {
    "sub_group": 1,
    "series": 2,
    "episode": 3,
    "tech_info": 4,
    "language": 5,
    "extension": 6
  },
  "output_format": "{series} S01E{episode} - [{sub_group}] [{tech_info}] [{language}]",
  "special_handling": {
    "year_pattern": "(19|20)\\d{2}",
    "max_episode": 999,
    "preserve_full_extension": true
  }
}
